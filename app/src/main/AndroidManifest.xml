<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.enrique.firstoriginal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_app"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirstOriginal">
        <activity
            android:name=".MainBottomNavigation"
            android:exported="false"
            android:label="@string/title_activity_main_bottom_navigation"
            android:theme="@style/Theme.FirstOriginal.NoActionBar" />
        <activity
            android:name=".Bottonbar"
            android:exported="false" />
        <activity
            android:name=".Signup"
            android:exported="true"
            android:parentActivityName=".MainLogin" />
        <activity
            android:name=".Splash"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".MainLogin"
            android:exported="true" />
    </application>

</manifest>